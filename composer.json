{
    "name": "tomodo531/multi-checkbox",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0|^9.0",
        "laravel/framework": "^8.0|^9.0",
        "laravel/nova": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Tomodo531\\MultiCheckbox\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tomodo531\\MultiCheckbox\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
